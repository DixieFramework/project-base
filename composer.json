{
    "type": "project",
    "license": "proprietary",
    "minimum-stability" : "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "api-platform/core": "^2.6",
        "babdev/pagerfanta-bundle": "^3.7",
        "beberlei/doctrineextensions": "^1.3",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.6",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.12",
        "exercise/htmlpurifier-bundle": "^4.1",
        "ezyang/htmlpurifier": "^4.16",
        "knplabs/knp-menu-bundle": "^3.2",
        "nelmio/cors-bundle": "^2.2",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.5",
        "ramsey/uuid-doctrine": "^1.8",
        "sabre/uri": "^2.2",
        "sensio/framework-extra-bundle": "^6.1",
        "stof/doctrine-extensions-bundle": "^1.7",
        "symfony/asset": "6.3.*",
        "symfony/console": "6.3.*",
        "symfony/doctrine-messenger": "6.3.*",
        "symfony/dotenv": "6.3.*",
        "symfony/expression-language": "6.3.*",
        "symfony/flex": "^2",
        "symfony/form": "6.3.*",
        "symfony/framework-bundle": "6.3.*",
        "symfony/http-client": "6.3.*",
        "symfony/intl": "6.3.*",
        "symfony/mailer": "6.3.*",
        "symfony/mime": "6.3.*",
        "symfony/monolog-bundle": "^3.0",
        "symfony/notifier": "6.3.*",
        "symfony/process": "6.3.*",
        "symfony/property-access": "6.3.*",
        "symfony/property-info": "6.3.*",
        "symfony/proxy-manager-bridge": "6.3.*",
        "symfony/runtime": "6.3.*",
        "symfony/security-bundle": "6.3.*",
        "symfony/serializer": "6.3.*",
        "symfony/string": "6.3.*",
        "symfony/translation": "6.3.*",
        "symfony/twig-bundle": "6.3.*",
        "symfony/validator": "6.3.*",
        "symfony/web-link": "6.3.*",
        "symfony/webapp-meta": "^1.0",
        "symfony/webpack-encore-bundle": "^1.14",
        "symfony/yaml": "6.3.*",
        "talav/bundle-generator-bundle": "@dev",
        "talav/core-bundle": "@dev",
        "talav/gallery-bundle": "@dev",
        "talav/permission-bundle": "@dev",
        "talav/profile-bundle": "@dev",
        "talav/settings-bundle": "@dev",
        "talav/user-bundle": "@dev",
        "talav/web-bundle": "@dev",
        "twig/extra-bundle": "^3.0",
        "twig/string-extra": "^3.7",
        "twig/twig": "^2.12|^3.0",
        "xemlock/htmlpurifier-html5": "^0.1.10"
    },
    "config": {
        "github-oauth": {

        },
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "php-http/discovery": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Groshy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Groshy\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.3.*"
        }
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^7.1",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "fakerphp/faker": "^1.19",
        "justinrainbow/json-schema": "^5.2",
        "phpunit/phpunit": "^9.5",
        "rector/rector": "^0.18.3",
        "symfony/browser-kit": "6.3.*",
        "symfony/css-selector": "6.3.*",
        "symfony/debug-bundle": "6.3.*",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^6.1",
        "symfony/stopwatch": "6.3.*",
        "symfony/web-profiler-bundle": "6.3.*",
        "symplify/easy-coding-standard": "^10.2"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:DixieFramework/Registry.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:DixieFramework/Resource.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:DixieFramework/ResourceBundle.git"
        },
        {
            "type": "path",
            "options": {
                "symlink": true
            },
            "url": "lib/Dixie/Bundle/BundleGeneratorBundle"
        },
        {
            "type": "path",
            "options": {
                "symlink": true
            },
            "url": "lib/Dixie/Bundle/CoreBundle"
        },
        {
            "type": "path",
            "options": {
                "symlink": true
            },
            "url": "lib/Dixie/Bundle/UserBundle"
        },
        {
            "type": "path",
            "options": {
                "symlink": true
            },
            "url": "lib/Dixie/Bundle/WebBundle"
        },
        {
            "type": "path",
            "options": {
                "symlink": true
            },
            "url": "lib/Dixie/Bundle/PermissionBundle"
        },
        {
            "type": "path",
            "options": {
                "symlink": true
            },
            "url": "lib/Dixie/Bundle/ProfileBundle"
        },
        {
            "type": "path",
            "options": {
                "symlink": true
            },
            "url": "lib/Dixie/Bundle/GalleryBundle"
        },
        {
            "type": "path",
            "options": {
                "symlink": true
            },
            "url": "lib/Dixie/Bundle/SettingsBundle"
        },
        {
            "type": "path",
            "options": {
                "symlink": true
            },
            "url": "lib/Dixie/Component/User"
        }
    ]
}
