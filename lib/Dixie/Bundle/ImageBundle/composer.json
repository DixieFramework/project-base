{
    "name": "talav/image-bundle",
    "description": "Symfony bundle providing image management functionality.",
    "keywords": ["symfony", "bundle", "image", "media", "php"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Talav",
            "email": "support@talav.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Talav\\ImageBundle\\": "src/"
        }
    },
    "require": {
        "php": ">=8.1.14",
        "ext-fileinfo": "*",
        "ext-gd": "*",
        "ext-intl": "*",
        "kornrunner/blurhash": "^1.2",
        "liip/imagine-bundle": "^2.0",
        "oneup/flysystem-bundle": "dev-allow_compatibility_with_flysysytem3#684c77ebe3507462f9129d39e1ff75cf89d37b2e"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "symfony/thanks": false
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/mihaileu/OneupFlysystemBundle.git"
        },
        {
            "type": "path",
            "url": "../../*/*"
        }
    ]
}
