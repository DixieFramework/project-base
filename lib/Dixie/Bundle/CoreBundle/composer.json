{
    "name": "talav/core-bundle",
    "version": "1.0.0",
    "description": "CoreBundle description",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Name",
            "homepage": "http://example.com/",
            "email": "example@example.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Talav\\CoreBundle\\": "src/"
        }
    },
    "require": {
        "php": ">=8.1.14"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "symfony/thanks": false
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../../*/*"
        }
    ]
}
