{
    "name": "talav/bundle-generator-bundle",
    "description": "Talav Bundle Generator",
    "keywords": ["symfony", "bundle", "generator", "php"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Talav",
            "email": "support@talav.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "symfony/finder": "^6.0",
        "symfony/config": "^6.0",
        "symfony/framework-bundle": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/phpunit-bridge": "^6.0",
        "symfony/browser-kit": "^6.0",
        "symfony/var-dumper": "^6.0",
        "symfony/console": "^6.0",
        "symfony/css-selector": "^6.0",
        "matthiasnoback/symfony-dependency-injection-test": "^4.3",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "fakerphp/faker": "^1.19",
        "liip/test-fixtures-bundle": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "Talav\\BundleGeneratorBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Talav\\BundleGeneratorBundle\\Tests\\": "tests/",
            "BundleGeneratorAppBundle\\": "tests/TestApp/src/BundleGeneratorAppBundle/"
        },
        "files": [
            "tests/TestApp/app/BundleGeneratorAppKernel.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../../*/*"
        }
    ]
}
