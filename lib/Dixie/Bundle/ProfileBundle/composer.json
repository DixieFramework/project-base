{
    "name": "talav/profile-bundle",
    "description": "ProfileBundle description",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Talav",
            "email": "support@talav.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Talav\\ProfileBundle\\": "src/"
        }
    },
    "require": {
        "php": ">=8.1.14",
        "twig/intl-extra": "^3.0",
        "twig/twig": "^3.0",
        "twig/extra-bundle": "^3.0"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "symfony/thanks": false
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../../*/*"
        }
    ]
}
