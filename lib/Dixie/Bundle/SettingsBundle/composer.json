{
    "name": "talav/settings-bundle",
    "description": "Symfony bundle to manage website settings",
    "keywords": ["symfony", "bundle", "settings", "php"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Talav",
            "email": "support@talav.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "talav/resource-bundle": "@dev",
        "symfony/form": "^6.0",
        "symfony/validator": "^6.0",
        "symfony/security-core": "^6.0",
        "sensio/framework-extra-bundle": "^6.0",
        "doctrine/orm": "^2.11",
        "doctrine/doctrine-bundle": "^2.6"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^8.0",
        "phpdocumentor/reflection-docblock": "^4.0",
        "symfony/framework-bundle": "^6.0",
        "symfony/debug": "^6.0@stable",
        "symfony/http-foundation": "^6.0",
        "symfony/security-bundle": "^6.0",
        "symfony/property-access": "^6.0",
        "symfony/monolog-bundle": "^3.4",
        "symfony/monolog-bridge": "^6.0",
        "symfony/serializer": "^6.0",
        "liip/functional-test-bundle": "^4.3",
        "symfony/yaml": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Talav\\SettingsBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Talav\\SettingsBundle\\Tests\\": "tests/",
            "SettingsAppBundle\\": "tests/TestApp/src/SettingsAppBundle/"
        },
        "files": [
            "tests/TestApp/app/SettingsAppKernel.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/mihaileu/OneupFlysystemBundle.git"
        },
        {
            "type": "path",
            "url": "../../*/*"
        }
    ]
}
